<section class="main-sec1">
    <section class="contact-us">
        <div class="container">
            <div class="row">
                <div class="col-md-9">
                    <div class="outer-section">
                        <div class="layer-content row">
                            <div class="col-md-10">
                                <div class="section-title">
                                    <h2>Send <strong>Us Message</strong></h2>
                                    <p>Please fill out the request for quote form below
                                        and our expert team will get back to you shortly.</p>
                                </div>
                                <form class="contact-form" method="post" action="/Contact-Us">
                                    <label class="form-content">
                                        <i class="fa fa-user" aria-hidden="true"></i>
                                        <input class="form-control" id="first_name" name="first_name" placeholder="Your Name:*" type="text" required="required" aria-invalid="false">
                                    </label>
                                    <label class="form-content">
                                        <i class="fa fa-envelope" aria-hidden="true"></i>
                                        <input class="form-control" id="last_name" name="email_id" placeholder="Your email-id:*" type="email" required="required" aria-invalid="false">
                                    </label>
                                    <label class="form-content">
                                        <i class="fa fa-envelope" aria-hidden="true"></i>
                                        <input class="form-control" id="subject" name="subject" placeholder="Your Subject" type="text" required="required" aria-invalid="false">
                                    </label>
                                    <label class="form-content">
                                        <i class="fa fa-comment"></i>
                                        <textarea class="form-control text-area" name="message" placeholder="Your Message:*" required="required"></textarea>
                                    </label>
                                    <div class="submit-btn">
                                        <button type="submit">Submit Now!</button>
                                    </div>
                                </form>
                            </div>
                            <div class="col-md-2"></div>
                        </div>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="drop-office">
                        <div class="inner-drop-office">
                            <h2>Drop in our office</h2>
                            <div class="left-icon">
                                <div class="featured-icon icon-white">
                                    <i class="fa fa-mobile" aria-hidden="true"></i>
                                    <div class="featured-content">
                                        <h5>Phone</h5>
                                        <p>+1800 200 133</p>
                                    </div>
                                </div>
                                <div class="featured-icon icon-white">
                                    <i class="fa fa-envelope-square"></i>
                                    <div class="featured-content">
                                        <h5>Email</h5>
                                        <p>abcd@example.com</p>
                                    </div>
                                </div>
                                <div class="featured-icon icon-white">
                                    <i class="fa fa-map-marker"></i>
                                    <div class="featured-content">
                                        <h5>Location</h5>
                                        <p>Envato HQ 24 Fifth st., Los Angeles, USA</p>
                                    </div>
                                </div>
                                <div class="featured-icon icon-white">
                                    <i class="fa fa-map-marker"></i>
                                    <div class="featured-content">
                                        <h5>Location 2</h5>
                                        <p>Envato HQ 24 Fifth st., Los Angeles, USA</p>
                                    </div>
                                </div>
                                <div class="social-icons">
                                    <ul class="list-item">
                                        <li class="hvr-grow">
                                            <a href=""><img src="img/facebook.svg"></a>
                                        </li>
                                        <li class="hvr-grow">
                                            <a href=""><img src="img/whatapp.svg"></a>
                                        </li>
                                        <li class="hvr-grow">
                                            <a href=""><img src="img/instagram.svg"></a>
                                        </li>
                                        <li class="hvr-grow">
                                            <a href=""><img src="img/youtube.svg"></a>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</section>
