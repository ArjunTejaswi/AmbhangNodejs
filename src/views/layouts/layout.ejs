<script type="text/javascript" src="js/owl.carousel.js"></script>
<script>
    function openNav() {
        document.getElementById("mySidenav").style.width = "calc(100% - 60px)";
        $('html,.no-overlay-display').attr("style","overflow:hidden;height:100%;");
        $('.filter-overlay').addClass("filter-open");
    }

    function closeNav() {
        document.getElementById("mySidenav").style.width = "0";
        $('html,.no-overlay-display').attr("style","overflow:auto;height:auto;");
        $('.filter-overlay').removeClass("filter-open");
    }
    $('#banner-slider').owlCarousel({
        items: 1,
        loop: true,
        autoplay:true,
        smartSpeed: 2000,
        autoplayTimeout: 6000,
        slideTransition: 'linear',
        autoplayHoverPause:false
    })
    $('#service-slider').owlCarousel({
        items: 1,
        loop:true,
        margin:20,
        nav:true,
        responsive: {
            0: {
                items: 1,
                margin: 10
            },
            575: {
                items: 2,
                margin: 15
            },
            767: {
                items: 2,
                margin: 25
            },
            1199: {
                items: 3,
                margin: 25
            }
        }
    })
    $('#testimonial').owlCarousel({
        items: 1,
        loop: true,
        autoplay:false,
        smartSpeed: 1000,
        autoplayTimeout: 6000,
        slideTransition: 'linear',
        autoplayHoverPause:false
    })
    $('#testimonial-slide').owlCarousel({
        items: 3,
        loop: true,
        autoplay:false,
        smartSpeed: 2000,
        autoplayTimeout: 6000,
        slideTransition: 'linear',
        autoplayHoverPause:false,
        responsive: {
            0: {
                items: 1,
                margin: 10
            },
            575: {
                items: 2,
                margin: 15
            },
            767: {
                items: 2,
                margin: 25
            },
            1199: {
                items: 3,
                margin: 25
            }
        }
    })
    $(".new-course").on("click", function () {
        if ($(".multi-level1").hasClass('open')) {
            $(".multi-level2").toggleClass("open");
        }
    });
    $(".new-course1").on("click", function () {
        if ($(".multi-level2").hasClass('open')) {
            $(".multi-level1").toggleClass("open");
        }
    });

</script>
